{
    "builds": [
        {
            "src": "BrandooBE/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "BrandooBE/main.py"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "BrandooBE/main.py" }]
}
